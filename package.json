{
	"name": "gatsby-source-notion-feely",
	"version": "0.0.1",
	"description": "Gatsby source plugin for official Notion.so API",
	"main": "index.js",
	"publishConfig": {
		"registry": "https://registry-url",
		"access": "public"
	},
	"scripts": {
		"test": "jest",
		"format": "prettier --write *.{js,json} **/*.{js,json} **/**/*.{js,json}",
		"lint": "prettier -c *.{js,json} **/*.{js,json} **/**/*.{js,json}",
		"commit": "gitmoji --commit",
		"build": "echo \"No build script setup\""
	},
	"keywords": [
		"gatsby",
		"gatsby-plugin",
		"notion"
	],
	"author": "Ethan",
	"license": "0BSD",
	"homepage": "https://github.com/dearlsh94/gatsby-source-notion-api",
	"bugs": {
		"email": "dearlsh94@gmail.com",
		"url": "https://github.com/dearlsh94/gatsby-source-notion-api/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/dearlsh94/gatsby-source-notion-api.git"
	},
	"dependencies": {
		"node-fetch": "^2.6.1",
		"or-pipets": "^1.0.1"
	},
	"devDependencies": {
		"babel-jest": "^29.7.0",
		"babel-preset-gatsby": "^3.12.0",
		"dotenv": "^16.3.1",
		"gitmoji-cli": "^4.1.0",
		"husky": "^6.0.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.7.0",
		"nyc": "^15.1.0",
		"prettier": "^2.3.0",
		"sinon": "^10.0.0",
		"yarn": "^1.22.10"
	},
	"husky": {
		"hooks": {
			"pre-push": "yarn lint"
		}
	},
	"prettier": {
		"printWidth": 100,
		"trailingComma": "all",
		"singleQuote": false,
		"semi": false,
		"useTabs": true,
		"tabWidth": 1,
		"bracketSpacing": true
	},
	"nyc": {
		"reporter": [
			"text",
			"lcov",
			"html"
		],
		"branches": 80,
		"lines": 80,
		"functions": 80,
		"statements": 80,
		"check-coverage": true,
		"all": true
	}
}
